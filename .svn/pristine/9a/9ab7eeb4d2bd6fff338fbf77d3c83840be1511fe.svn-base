package ch1;

public class Static_initialization_block {

	public static int somevalue = 10;
	
	static {
		System.out.println("static initializer 1");
	}
	
	{
		System.out.println("instance initializer 1");
	}

	public Static_initialization_block() {
		System.out.println("constructor");
	}

	{
		System.out.println("instance initializer 2");
	}
	
	static {
		System.out.println("static initializer 2");
	}
	
	
	public static void main(String args[]){
		System.out.println(Static_initialization_block.somevalue);
		Static_initialization_block sib1 = new  Static_initialization_block();
		Static_initialization_block sib2 = new  Static_initialization_block();
		Static_initialization_block sib3 = new  Static_initialization_block();
	}

}
